{
  "name": "jwt-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www"
  },
  "apidoc": {
    "title": "Documentação da API",
    "description": "Esta documentação descreve os recursos disponibilizados pela API. Caso algum recurso retorne um erro 5xx ou a documentação possua alguma discordância entre em contato com o administrador.",
    "url": "http://<api_domain>:<port>",
    "order": [
      "Teste",
      "Login",
      "Logout",
      "Users",
      "News",
      "Listar",
      "Recuperar",
      "Criar",
      "Atualizar",
      "Excluir",
      "CriarImagens",
      "AtualizarImagens",
      "ExcluirImagem"
    ],
    "template": {
      "withCompare": false,
      "withGenerator": false
    }
  },
  "dependencies": {
    "async": "^1.5.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-validator": "^2.18.0",
    "fs": "0.0.2",
    "jwt-simple": "^0.4.0",
    "lodash": "^3.10.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.10.6",
    "mongoose": "^4.3.3",
    "mongoose-paginate": "^5.0.0",
    "mongoose-slug-generator": "^1.0.2",
    "mongoose-validator": "^1.2.4",
    "morgan": "~1.6.1",
    "multiparty": "^4.1.2",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "apidoc": "^0.14.0",
    "gulp": "^3.9.0",
    "gulp-apidoc": "^0.2.2"
  }
}
